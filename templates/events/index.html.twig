{% extends 'base.html.twig' %}

{% block title %}Hello EventsController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
</style>

<div class="example-wrapper">
    <h1>{{ events|length }} Events</h1>

    <ul>
    {% for event in events %}
        <li>{{ event }}</li>
    {% endfor %}
    </ul>
</div>
{% endblock %}
